//奇数在前偶数在后
void x(int* arr)
 {
	int i = 0;
	int n = 0;
	int tmp = 0;
	
	 for (i = 0; i < 10; i++)
	 {
		 if (arr[i] % 2 == 1)
		 {
			tmp = arr[n];
			arr[n] = arr[i];
			arr[i] = tmp;
			//不是奇数的情况下n不++
			 n++;
		 }
	 }
}
int main()
{
	int arr[] = { 1,2,3,4,4,5,6,7,8,9,10 };
	x(arr);
	int i = 0;
	for (i = 0; i < 10; i++)
	{
		printf("%d ", arr[i]);
	}
	 return 0;
}
