void left_move(char* str ,int k)
{
	int i = 0;
	int len = strlen(str);
	for (i = 0; i < k; i++)
	{
		//左旋转一个字符
		//1
		char tmp = *str;
		//2
		int j = 0;
		for (j = 0; j < len-1; j++)
		{
			*(str + j) = *(str + j + 1);
		}
		//3
		*(str + len - 1) = tmp;
	}
}
int main()
{
	char arr[] = "abcdef";
	int k;
	scanf("%d", &k);
	left_move(arr, k);
	printf("%s\n", arr);
	return 0;
}
