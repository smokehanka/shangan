int My_Strcmp(const char* str1,const char* str2)
{
	assert(str1 && str2);
	//æ¯”è¾ƒ
	while (*str1==*str2)
	{
		if (*str1 == '\0')
		{
			return 0;
		}
		str1++;
		str2++;
	}
	if (*str1 > *str2)
		return 1;
	else
		return -1;
}
int main()
{
	char* p1 = "a";
	char* p2 = "c";
	int ret = My_Strcmp(p1,p2);
	printf("%d\n", ret);
	return 0;
}
