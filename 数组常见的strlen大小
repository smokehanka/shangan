int main()
{
	char arr[] = { 'a','b','c','d','e','f' };
	printf("%d\n", strlen(arr));//随机值x
	printf("%d\n", strlen(arr + 0));//随机值x
	//printf("%d\n", strlen(*arr));//error
	//printf("%d\n", strlen(arr[1]));//error
	printf("%d\n", strlen(&arr));//随机值x
	printf("%d\n", strlen(&arr + 1));//随机值x-6
	printf("%d\n", strlen(&arr[0] + 1));//随机值x-1
	return 0;
}
